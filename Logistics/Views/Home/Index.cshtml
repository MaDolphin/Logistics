@model Logistics.Models.Process
@{
    ViewBag.Title = "Home Page";
}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title></title>
    <link href="http://libs.baidu.com/bootstrap/3.0.3/css/bootstrap.min.css" rel="stylesheet">
    <link href="../media/css/PackNoform.css" rel="stylesheet" type="text/css" />

    <script src="http://libs.baidu.com/jquery/2.0.0/jquery.min.js"></script>
    <script src="http://libs.baidu.com/bootstrap/3.0.3/js/bootstrap.min.js"></script>
</head>
<body style="margin:0px 20px 0px 20px; ">
    <nav class="navbar navbar-inverse" role="navigation">
        <div class="navbar-header">
            <a class="navbar-brand" href="#">快递100</a>
        </div>
        <div>
            <ul class="nav navbar-nav navbar-right">
                <li><a href="~/CoustomerModels/Complaint">投诉</a></li>
                <li class="active"><a href="~/Home/Login">登录</a></li>
                <li style="width:40px;"></li>
            </ul>
        </div>
    </nav>

    <div style="margin:40px 0 100px 0;background: white">
        <div class="form-wrapper cf">
            @using (Html.BeginForm("Process", "CoustomerModels", FormMethod.Post))
            {
                @Html.AntiForgeryToken()
                @Html.EditorFor(model => model.PackNo, new { htmlAttributes = new { @id= "tags", @placeholder = "快递单号..." } })
                <button type="submit">查找快递</button>
            }
        </div>
    </div>

    <div style="margin:0px 150px 0px 150px">
        <div id="myCarousel" class="carousel slide">
            <!-- 轮播（Carousel）指标 -->
            <ol class="carousel-indicators">
                <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
                <li data-target="#myCarousel" data-slide-to="1"></li>
                <li data-target="#myCarousel" data-slide-to="2"></li>
            </ol>
            <!-- 轮播（Carousel）项目 -->
            <div class="carousel-inner">
                <div class="item active">
                    <img src="../Image/index1.jpg" alt="First slide">
                </div>
                <div class="item">
                    <img src="../Image/index2.jpg" alt="Second slide">
                </div>
                <div class="item">
                    <img src="../Image/index3.jpg" alt="Third slide">
                </div>
            </div>
            <!-- 轮播（Carousel）导航 -->
            <a class="carousel-control left" href="#myCarousel"
               data-slide="prev">&lsaquo;</a>
            <a class="carousel-control right" href="#myCarousel"
               data-slide="next">&rsaquo;</a>
        </div>

        <h1><b>快递</b></h1>
        <p class="lead"><b>    快递能够被</b></p>
    </div>

</body>
</html>
